{
    "name": "zirsakht/laravel-responder",
    "description": "A Responder using JSON & XML for Laravel",
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "Zirsakht\\Responder\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zirsakht\\Responder\\ResponderServiceProvider"
            ],
            "aliases": {
                "Responder": "Zirsakht\\Responder\\Facades\\Responder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
